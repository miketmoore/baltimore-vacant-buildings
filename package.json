    "body-parser": "~1.13.2",
    "bower": "^1.7.9",
    "cookie-parser": "~1.3.5",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "react": "^15.0.2",
    "react-data-grid": "^0.14.35",
    "react-dom": "^15.0.2",
    "react-router-component": "^0.32.2",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
